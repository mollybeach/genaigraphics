---
// path: /src/components/primitives/Link.astro
import Svg from "./Svg.astro";
import {classNames} from "../../styles/classStyles.js";
const { route} = Astro.props as {
    type: string; 
};
---
<a
  id="primitives-link"
  href={route || "#!"}
  type="button"
  class={classNames("primitivesLink")}
  data-te-ripple-in="dark"
  data-te-ripple-color="light"
>
  <slot/>
</a>